{% extends 'base.html.twig' %}

{% block bodyAtt %}class="fixed-left"{% endblock %}

{% block body %}

                    <div class="row">
                        <div class="col-xs-12">
                            <div class="page-title-box">
                                <h4 class="page-title"><i class="mdi mdi-phone-incoming"></i> Nuevo implementación
                                    detalle
                                </h4>
                                <div class="clearfix"></div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="card-box">
                                <h4 class="m-t-0 header-title"><b>Información.</b></h4>
                                <hr>
                                <div class="row">
                                    <div class="col-md-12">

                                        {{ form_start(form, { 'attr': {'id':'form','class': 'form-horizontal','data-parsley-validate':'true'} }) }}
                                        <div class="form-group">
                                            <label class="col-md-2 control-label">Tema:</label>
                                            <div class="col-md-4">
                                                {{ form_widget(form.implementacionTemaRel) }}
                                            </div>
                                            <label class="col-md-2 control-label">Orden:</label>
                                            <div class="col-md-4">
                                                {{ form_widget(form.orden) }}
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-2 control-label">Fecha capacitación:</label>
                                            <div class="col-md-4">
                                                {{ form_widget(form.fechaCapacitacion) }}
                                            </div>
                                        </div>
                                        <hr/>
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                {{ form_widget(form.btnGuardar, { 'attr': {'class': 'btn btn-success btn-bordered waves-effect w-md waves-light m-b-5','style':'float:right;margin-left:5px;'} }) }}
                                            </div>
                                        </div>
                                        {{ form_rest(form) }}
                                        {{ form_end(form) }}
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

        {{ include('bloquesGenerales/pie.html.twig') }}

{% endblock %}